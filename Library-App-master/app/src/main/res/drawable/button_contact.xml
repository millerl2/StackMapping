<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle" >
            <corners
                android:radius="15dp"
                />
            <solid
                android:color="#056d99"
                />
            <size
                android:width="250dp"
                android:height="60dp"
                />
            <stroke
                android:width="5dp"
                android:color="#043e7e"
                />
        </shape>
    </item>
    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle" >
            <corners
                android:radius="15dp"
                />
            <solid
                android:color="#0694cf"
                />
            <size
                android:width="250dp"
                android:height="60dp"
                />
            <stroke
                android:width="5dp"
                android:color="#043e7e"
                />
        </shape>
    </item>
</selector>
